{% extends 'templateadmin.html.twig' %}



{% block script_calcul %}

{% endblock %}

{% block body %}



<div class="mdl-grid demo-content">

<div class="mdl-cell mdl-cell--12-col mdl-grid" style="padding:20px;">
<div class="mdl-cell mdl-cell--4-col">
</div>
<div class="demo-graphs mdl-shadow--2dp mdl-color--white mdl-cell mdl-cell--4-col">
<center>

<div style="padding-bottom:20px;"><h5>Ajouter un Atelier</h5></div>
{{ form_start(form, {'attr': { 'class': '' } }) }}

<div class="form-group">
    {{ form_label(form.name, "Nom de l'atelier", {'label_attr': {'class': ''}}) }}
    {{ form_errors(form.name) }}
    {{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}

</div>
<div class="form-group">
    {{ form_label(form.franchise, "Franchisé", {'label_attr': {'class': ''}}) }}
    {{ form_errors(form.franchise) }}
    {{ form_widget(form.franchise, {'attr': {'class': 'form-control', 'disabled':'disabled'}}) }}

</div>


<div class="form-group">
    {{ form_label(form.adresse1, "Adresse", {'label_attr': {'class': ''}}) }}
    {{ form_errors(form.adresse1) }}
    {{ form_widget(form.adresse1, {'attr': {'class': 'form-control'}}) }}

</div>


<div class="form-group">
    {{ form_label(form.adresse2, "Adresse suite", {'label_attr': {'class': ''}}) }}
    {{ form_errors(form.adresse2) }}
    {{ form_widget(form.adresse2, {'attr': {'class': 'form-control'}}) }}

</div>

<div class="form-group">
    {{ form_label(form.zipcode, "Code Postal", {'label_attr': {'class': ''}}) }}
    {{ form_errors(form.zipcode) }}
    {{ form_widget(form.zipcode, {'attr': {'class': 'form-control'}}) }}

</div>


<div class="form-group">
    {{ form_label(form.city, "Ville", {'label_attr': {'class': ''}}) }}
    {{ form_errors(form.city) }}
    {{ form_widget(form.city, {'attr': {'class': 'form-control'}}) }}

</div>


<div class="form-group">
    {{ form_label(form.phone , "Téléphone", {'label_attr': {'class': ''}}) }}
    {{ form_errors(form.phone) }}
    {{ form_widget(form.phone, {'attr': {'class': 'form-control'}}) }}

</div>
<div class="form-group">
    {{ form_label(form.email , "Email", {'label_attr': {'class': ''}}) }}
    {{ form_errors(form.email) }}
    {{ form_widget(form.email, {'attr': {'class': 'form-control'}}) }}

</div>

<div>
        <input type="submit" value="Valider" class="btn btn-primary" />
</div>
{{ form_end(form) }}
</center>
</div>
<div class=" mdl-cell mdl-cell--4-col">
</div>
</div>



{% endblock %}

