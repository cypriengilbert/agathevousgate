{% extends 'templateadmin.html.twig' %}



{% block script_calcul %}

{% endblock %}

{% block body %}



<div class="mdl-grid demo-content">
<div><h4>Modifier une commande</h4></div>
  <div class="demo-charts mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid" style="padding:20px;">
  {{ form_start(form, {'method': 'post'}) }}
   {{ form_errors(form) }}

   <div class="form-group">
       {{ form_label(form.transportMethod, "Transport", {'label_attr': {'class': ''}}) }}
       {{ form_errors(form.transportMethod) }}
       {{ form_widget(form.transportMethod, {'attr': {'class': 'form-control'}}) }}
   </div>
   {{ form_row(form.client) }}


      <div>
          <button type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-primary" style="margin-bottom:10px;">Modifier</button>
      </div>
  {{ form_end(form) }}

  </div>




{% endblock %}

