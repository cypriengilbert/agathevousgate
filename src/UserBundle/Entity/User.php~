<?php

namespace UserBundle\Entity;

use FOS\UserBundle\Model\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;


/**
 * User
 *
 * @ORM\Table(name="user")
 * @ORM\Entity(repositoryClass="UserBundle\Repository\UserRepository")
 */
class User extends BaseUser
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @var string
     *
     * @ORM\Column(name="nom", type="string", length=255)
     */
    private $nom;

    /**
     * @var string
     *
     * @ORM\Column(name="prenom", type="string", length=255)
     */
    private $prenom;

     /**
      * @ORM\ManyToOne(targetEntity="UserBundle\Entity\UserAdress", cascade={"persist"})
      * @ORM\JoinColumn(nullable=false)
      */
     private $adress;


      /**
      * @ORM\ManyToOne(targetEntity="UserBundle\Entity\User")
      * @ORM\JoinColumn(nullable=true)
      */
     private $parrain;


     /**
      * @var string
      *
      * @ORM\Column(name="genre", type="string", length=255, nullable=true)
      */
     private $genre;

     /**
      * @var integer
      *
      * @ORM\Column(name="naissance", type="integer", nullable=true)
      */
     private $naissance;

     /**
      * @var bool
      *
      * @ORM\Column(name="is_pro", type="boolean")
      */
     private $isPro;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nom
     *
     * @param string $nom
     * @return User
     */
    public function setNom($nom)
    {
        $this->nom = $nom;

        return $this;
    }

    /**
     * Get nom
     *
     * @return string
     */
    public function getNom()
    {
        return $this->nom;
    }

    /**
     * Set prenom
     *
     * @param string $prenom
     * @return User
     */
    public function setPrenom($prenom)
    {
        $this->prenom = $prenom;

        return $this;
    }

    /**
     * Get prenom
     *
     * @return string
     */
    public function getPrenom()
    {
        return $this->prenom;
    }

    //adress
    public function setAdress(UserAdress $adress)
      {
        $this->adress = $adress;

        return $this;
      }

      public function getAdress()
      {
        return $this->adress;
      }

//parrain
        public function setParrain(User $parrain)
      {
        $this->parrain = $parrain;

        return $this;
      }

      public function getParrain()
      {
        return $this->parrain;
      }

    /**
     * Set genre
     *
     * @param string $genre
     * @return User
     */
    public function setGenre($genre)
    {
        $this->genre = $genre;

        return $this;
    }

    /**
     * Get genre
     *
     * @return string
     */
    public function getGenre()
    {
        return $this->genre;
    }

    /**
     * Set naissance
     *
     * @param integer $naissance
     * @return User
     */
    public function setNaissance($naissance)
    {
        $this->naissance = $naissance;

        return $this;
    }

    /**
     * Get naissance
     *
     * @return integer
     */
    public function getNaissance()
    {
        return $this->naissance;
    }
}
